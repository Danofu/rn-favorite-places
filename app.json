{
  "expo": {
    "android": { "adaptiveIcon": { "backgroundColor": "#ffffff", "foregroundImage": "./assets/adaptive-icon.png" } },
    "assetBundlePatterns": ["**/*"],
    "icon": "./assets/icon.png",
    "ios": { "supportsTablet": true },
    "name": "FavoritePlaces",
    "orientation": "portrait",
    "plugins": [
      [
        "expo-image-picker",
        { "cameraPermission": "The app needs access to your camera in order to take photos of your favourite places." }
      ]
    ],
    "slug": "FavoritePlaces",
    "splash": { "backgroundColor": "#ffffff", "image": "./assets/splash.png", "resizeMode": "contain" },
    "userInterfaceStyle": "light",
    "version": "1.0.0",
    "web": { "favicon": "./assets/favicon.png" }
  }
}
